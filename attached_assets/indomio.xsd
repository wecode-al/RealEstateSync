<?xml version="1.0" encoding="utf-8" ?>
<xs:schema xmlns:vc="http://www.w3.org/2007/XMLSchema-versioning" attributeFormDefault="unqualified"
    elementFormDefault="qualified" vc:minVersion="1.1" xmlns:xs="http://www.w3.org/2001/XMLSchema">
    <xs:element name="properties">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="property" maxOccurs="unbounded"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="property">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="mainFeatures" type="mainFeatures"/>
                <xs:element name="location" type="location"/>
                <xs:element name="categoryFeatures">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="propertyType">
                                <xs:alternative test="@category = 'residential'"
                                    type="house_category"/>
                                <xs:alternative test="@category = 'commercial'"
                                    type="office_category"/>
                                <xs:alternative test="@category = 'land'" type="land_category"/>
                                <xs:alternative test="@category = 'other'" type="other_category"/>
                                <xs:alternative type="xs:error"/>
                            </xs:element>
                            <xs:element name="originalPropertyType" minOccurs="0">
                                <xs:annotation>
                                    <xs:documentation>
                                        The type of the property as specified in your system. e.g. penthouse or house of character.
                                    </xs:documentation>
                                </xs:annotation>
                                <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                        <xs:maxLength value="50"/>
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:element>
                            <xs:element name="residential_features" type="residential_features"
                                minOccurs="0"/>
                            <xs:element name="commercial_features" type="commercial_features"
                                minOccurs="0"/>
                            <xs:element name="land_features" type="land_features" minOccurs="0"/>
                            <xs:element name="otherProperty_features" type="otherProperty_features"
                                minOccurs="0"/>
                        </xs:sequence>
                        <xs:assert
                            test="not(propertyType/attribute::category = 'residential') or (residential_features and not(commercial_features) and not(land_features) and not(otherProperty_features))"/>
                        <xs:assert
                            test="not(propertyType/attribute::category = 'commercial') or (commercial_features and not(residential_features) and not(land_features) and not(otherProperty_features))"/>
                        <xs:assert
                            test="not(propertyType/attribute::category = 'land') or (land_features and not(residential_features) and not(commercial_features) and not(otherProperty_features))"/>
                        <xs:assert
                            test="not(propertyType/attribute::category = 'other') or (otherProperty_features and not(residential_features) and not(commercial_features) and not(land_features))"
                        />
                    </xs:complexType>
                </xs:element>
                <xs:element ref="descriptions" minOccurs="0"/>
                <xs:element name="media" minOccurs="0" type="media"/>
                <xs:element name="agent" minOccurs="0" type="agent"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="photo">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="url">
                    <xs:simpleType>
                        <xs:restriction base="xs:string">
                            <xs:maxLength value="500"/>
                            <xs:minLength value="10"/>
                        </xs:restriction>
                    </xs:simpleType>
                </xs:element>
                <xs:element name="priority">
                    <xs:annotation>
                        <xs:documentation>
                            25 photos are displayed per listing (order by priority asc). 
                        </xs:documentation>
                    </xs:annotation>
                    <xs:simpleType>
                        <xs:restriction base="xs:unsignedByte">
                            <xs:minInclusive value="1"/>
                            <xs:maxInclusive value="100"/>
                        </xs:restriction>
                    </xs:simpleType>
                </xs:element>
                <xs:element ref="photoDescriptions" minOccurs="0"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="description" type="descriptionType"/>
    <xs:element name="address" type="addressType"/>
    <xs:element name="shortDescription" type="shortDescriptionType"/>
    <xs:element name="photoDescription" type="photoDescriptionType"/>
    <xs:complexType name="location">
        <xs:sequence>
            <xs:element name="geographyId" nillable="false">
                <xs:simpleType>
                    <xs:annotation>
                        <xs:documentation>
                            It is the id of the level-2 or level-3 geography (see list of supported geographies). Level-1 geographies are not allowed.
                        </xs:documentation>
                    </xs:annotation>
                    <xs:restriction base="xs:unsignedInt">
                        <xs:minInclusive value="0"/>
                        <xs:maxInclusive value="2147483647"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="originalLocation" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                        The location of the property as specified in your system. The full path should be specified, e.g. Primorsko-goranska/Rijeka/Gornji Zamet
                    </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:maxLength value="100"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element ref="addresses" minOccurs="0"/>
            <xs:element name="geocodes" type="geocodes" minOccurs="0"/>
            <xs:element name="zip" minOccurs="0">
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:maxLength value="10"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="zoning" minOccurs="0" type="zones"> </xs:element>
            <xs:element name="roadType" minOccurs="0" type="road_types"/>
        </xs:sequence>
    </xs:complexType>
    <xs:element name="youtubeURL">
        <xs:annotation>
            <xs:documentation>            
            A URL to a video or other presentation of the property in youtube.
            </xs:documentation>
        </xs:annotation>
        <xs:simpleType>
            <xs:restriction base="xs:string">
                <xs:maxLength value="255"/>
                <xs:minLength value="1"/>
            </xs:restriction>
        </xs:simpleType>
    </xs:element>
    <xs:complexType name="media">
        <xs:sequence>
            <xs:element ref="photos" minOccurs="0"/>
            <xs:element ref="youtubeURL" minOccurs="0"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="mainFeatures">
        <xs:sequence>
            <xs:element name="id" type="xs:long"/>
            <xs:element name="brokerListingID" minOccurs="0">
                <xs:simpleType>
                    <xs:annotation>
                        <xs:documentation>
                            Listing code as set by the broker or his own CRM/listing management software. 
                        </xs:documentation>
                    </xs:annotation>
                    <xs:restriction base="xs:string">
                        <xs:maxLength value="20"/>
                        <xs:minLength value="1"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="dateUpdated" type="xs:date">
                <xs:annotation>
                    <xs:documentation>
                        When the property listing was last updated in your system. 
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="dateAvailable">
                <xs:simpleType>
                    <xs:annotation>
                        <xs:documentation>
                            Date that the property becomes available. If already available this should be set to a past date. If not provided will automatically set to yesterday's date.
                        </xs:documentation>
                    </xs:annotation>
                    <xs:restriction base="xs:date">
                        <xs:minInclusive value="2000-01-01"/>
                        <xs:maxInclusive value="2050-12-31"/>
                        <xs:pattern value="\p{Nd}{4}-\p{Nd}{2}-\p{Nd}{2}"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="size" type="xs:unsignedInt" default="0">
                <xs:annotation>
                    <xs:documentation>
                        The size of the property in square meters. External areas not to be included.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="listingType" default="for sale" type="listing_types"> </xs:element>
            <xs:element name="price" type="priceType"/>
            <xs:element name="investment" minOccurs="0" type="xs:boolean">
                <xs:annotation>
                    <xs:documentation>
                        True if the property is suitable for investment purposes.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="corner" type="xs:boolean" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                        Whether the property is situated in a corner.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="view" type="view_types" minOccurs="0"/>
            <xs:element name="noAgentFee" default="false" type="xs:boolean">
                <xs:annotation>
                    <xs:documentation>
                        True if the agency will not charge the buyer / tenant with commission.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="descriptionType">
        <xs:sequence>
            <xs:element name="language" type="languages"/>
            <xs:element name="description" nillable="false">
                <xs:simpleType>
                    <xs:annotation>
                        <xs:documentation>
                            Description of the property in the activated languages. NOTE: no HTML is allowed.
                        </xs:documentation>
                    </xs:annotation>
                    <xs:restriction base="xs:string">
                        <xs:maxLength value="5000"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="shortDescriptionType">
        <xs:sequence>
            <xs:element name="language" type="languages"/>
            <xs:element name="description" nillable="false">
                <xs:annotation>
                    <xs:documentation>
                        Short description of the property in the activated languages. NOTE: no HTML is allowed.
                    </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:maxLength value="75"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="photoDescriptionType">
        <xs:sequence>
            <xs:element name="language" type="languages"/>
            <xs:element name="description" nillable="false">
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:maxLength value="250"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="addressType">
        <xs:sequence>
            <xs:element name="language" type="languages"/>
            <xs:element name="streetAddress" nillable="false">
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:maxLength value="65535"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="priceType">
        <xs:sequence>
            <xs:element name="amount" type="xs:unsignedInt" default="0"/>
            <xs:element name="currency" default="EUR" type="currencies"/>
        </xs:sequence>
    </xs:complexType>
    <xs:element name="photos">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="photo" minOccurs="0" maxOccurs="unbounded"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="descriptions">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="description" minOccurs="0" maxOccurs="unbounded"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>    
    <xs:element name="photoDescriptions">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="photoDescription" minOccurs="0" maxOccurs="unbounded"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="addresses">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="address" minOccurs="0" maxOccurs="unbounded"/>
                <xs:element name="addressVisible" default="false" type="xs:boolean">
                    <xs:annotation>
                        <xs:documentation>
                            If the address will be displayed to the end-user or not.
                        </xs:documentation>
                    </xs:annotation>
                </xs:element>
            </xs:sequence>
        </xs:complexType>
    </xs:element>

    <xs:complexType name="geocodes">
        <xs:sequence>
            <xs:element name="latitude" minOccurs="0">
                <xs:simpleType>
                    <xs:annotation>
                        <xs:documentation>
                            Latitude for optionally showing the exact location of the property on map.						
                        </xs:documentation>
                    </xs:annotation>
                    <xs:restriction base="xs:float">
                        <xs:minInclusive value="-180"/>
                        <xs:maxInclusive value="180"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="longitude" minOccurs="0">
                <xs:simpleType>
                    <xs:annotation>
                        <xs:documentation>
                            Latitude for optionally showing the exact location of the property on map.
                        </xs:documentation>
                    </xs:annotation>
                    <xs:restriction base="xs:float">
                        <xs:minInclusive value="-3.402823e+38"/>
                        <xs:maxInclusive value="3.402823e+38"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>    
    <xs:complexType name="agent">
        <xs:annotation>
            <xs:documentation>                        
                The responsible agent / employee for the property. Use this block to display different contact details per listing.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="firstName">
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:maxLength value="35"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="lastName">
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:maxLength value="35"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="phoneNumber">
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:maxLength value="25"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="email">
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:maxLength value="50"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:simpleType name="languages">
        <xs:restriction base="xs:string">
            <xs:enumeration value="gr"/>
            <!-- Greek -->
            <xs:enumeration value="en"/>
            <!-- English -->
            <xs:enumeration value="cs"/>
            <!-- Czech -->
            <xs:enumeration value="ru"/>
            <!-- Russian -->
            <xs:enumeration value="de"/>
            <!-- German -->
            <xs:enumeration value="bg"/>
            <!-- Bulgarian -->
            <xs:enumeration value="fr"/>
            <!-- French -->
            <xs:enumeration value="es"/>
            <!-- Spanish -->
            <xs:enumeration value="tr"/>
            <!-- Turkish -->
            <xs:enumeration value="pl"/>
            <!-- Polish -->
            <xs:enumeration value="sr"/>
            <!-- Serbian -->
            <xs:enumeration value="zh"/>
            <!-- Chinese -->
            <xs:enumeration value="nl"/>
            <!-- Dutch -->
            <xs:enumeration value="ar"/>
            <!-- Arabic -->
            <xs:enumeration value="ro"/>
            <!-- Romanian -->
            <xs:enumeration value="me"/>
            <!-- Montenegrin -->
            <xs:enumeration value="mt"/>
            <!-- Maltese -->
            <xs:enumeration value="bs"/>
            <!-- Bosnian -->
            <xs:enumeration value="hr"/>
            <!-- Croatian -->
            <xs:enumeration value="sr"/>
            <!-- Slovenian -->
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="mainPropertyCategories">
        <xs:restriction base="xs:string">
            <xs:enumeration value="residential"/>
            <xs:enumeration value="commercial"/>
            <xs:enumeration value="land"/>
            <xs:enumeration value="other"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:complexType name="house_category">
        <xs:attribute name="category" type="xs:string" use="required"/>
        <xs:attribute name="subCategory" type="house_types" use="required"/>
    </xs:complexType>
    <xs:complexType name="office_category">
        <xs:attribute name="category" type="xs:string" use="required"/>
        <xs:attribute name="subCategory" type="office_types" use="required"/>
    </xs:complexType>
    <xs:complexType name="land_category">
        <xs:attribute name="category" type="xs:string" use="required"/>
        <xs:attribute name="subCategory" type="land_types" use="required"/>
    </xs:complexType>
    <xs:complexType name="other_category">
        <xs:attribute name="category" type="xs:string" use="required"/>
        <xs:attribute name="subCategory" type="other_types" use="required"/>
    </xs:complexType>
    <xs:simpleType name="house_types">
        <xs:restriction base="xs:NMTOKENS">
            <xs:enumeration value="apartment"/>
            <xs:enumeration value="studio"/>
            <xs:enumeration value="maisonette"/>
            <xs:enumeration value="detached"/>
            <xs:enumeration value="villa"/>
            <xs:enumeration value="loft"/>
            <xs:enumeration value="bungalow"/>
            <xs:enumeration value="building"/>
            <xs:enumeration value="apartment complex"/>
            <xs:enumeration value="farm"/>
            <xs:enumeration value="houseboat"/>
            <xs:enumeration value="other residential"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="office_types">
        <xs:restriction base="xs:NMTOKENS">
            <xs:enumeration value="office"/>
            <xs:enumeration value="store"/>
            <xs:enumeration value="warehouse"/>
            <xs:enumeration value="industrial space"/>
            <xs:enumeration value="craft space"/>
            <xs:enumeration value="hotel"/>
            <xs:enumeration value="business building"/>
            <xs:enumeration value="hall"/>
            <xs:enumeration value="showroom"/>
            <xs:enumeration value="other commercial"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="land_types">
        <xs:restriction base="xs:NMTOKENS">
            <xs:enumeration value="plot"/>
            <xs:enumeration value="parcel"/>
            <xs:enumeration value="island"/>
            <xs:enumeration value="other land"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="other_types">
        <xs:restriction base="xs:NMTOKENS">
            <xs:enumeration value="parking spot"/>
            <xs:enumeration value="business"/>
            <xs:enumeration value="prefabricated"/>
            <xs:enumeration value="air"/>
            <xs:enumeration value="other"/>
        </xs:restriction>
    </xs:simpleType>

