    <xs:simpleType name="currentTypes">
        <xs:restriction base="xs:string">
            <xs:enumeration value="single phase"/>
            <xs:enumeration value="three phase"/>
            <xs:enumeration value="industrial"/>
            <xs:enumeration value="none"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="heatingControllers">
        <xs:restriction base="xs:string">
            <xs:enumeration value="individual"/>
            <xs:enumeration value="autonomous"/>
            <xs:enumeration value="central"/>
            <xs:enumeration value="none"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="heatingMediums">
        <xs:restriction base="xs:string">
            <xs:enumeration value="petrol"/>
            <xs:enumeration value="natural gas"/>
